<Window x:Name="___Sin_nombre_" x:Class="IndiceAcademico.mainwindows.RankingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IndiceAcademico.mainwindows"
        mc:Ignorable="d" Height="450" Width="800">
    <Grid>
        <Label Content="Ranking de Indices" Margin="37,24,0,0" VerticalAlignment="Top" Background="{x:Null}" FontSize="22" FontFamily="MS Reference Sans Serif" Foreground="Black" HorizontalAlignment="Left" Width="228"/>
        <Rectangle Fill="#FFFF2D2D" Height="4" Margin="40,65,40,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
        </Rectangle>
        <Button Content="Mostrar Indices" HorizontalAlignment="Left" VerticalAlignment="Top" Width="119" Margin="633,36,0,0" Click="Button_Click"/>
        <DataGrid x:Name="DataGrid" HorizontalAlignment="Left" Height="335" VerticalAlignment="Top" Width="712" RenderTransformOrigin="0.5,0.51" Margin="40,74,0,0" SelectionChanged="DataGrid_SelectionChanged" IsReadOnly="True"/>
    </Grid>
</Window>
